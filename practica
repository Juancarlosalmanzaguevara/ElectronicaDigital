#include <stdio.h>

int main() {
    printf("Potencias de 2 entre el 1 y el 100:\n");
    
    // uno por uno, del 1 al 100
    for (int i = 1; i <= 100; i++) {
        
        
        if (i > 0 && (i & (i - 1)) == 0) {
            
            // Si se cumple:
            printf("%d\n", i);
        }
    }
    
    return 0;
}
